{
  "name": "game-creator",
  "version": "1.0.0",
  "description": "Electron game",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:windows": "electron-packager . YourGame --overwrite --platform=win32 --arch=x64 --prune=true --out=out",
    "build:linux": "electron-packager . YourGame --overwrite --platform=linux --arch=x64 --prune=true --out=out",
    "build:mac": "electron-packager . YourGame --overwrite --platform=darwin --arch=x64 --prune=true --out=out"
  },
  "keywords": [],
  "author": "ashikuri",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^21.2.0",
    "electron-packager": "^14.0.6"
  },
  "dependencies": {
    "ok-enum": "^1.0.5",
    "spiel-engine": "^1.0.3"
  }
}
